{
  "name": "eslint-plugin-patternplate",
  "version": "1.0.0",
  "description": "Integrate patternplate with eslint",
  "main": "library/index.js",
  "files": [
    "library"
  ],
  "scripts": {
    "start": "npm run build -- --watch",
    "test": "ava",
    "build": "babel source -d library",
    "lint": "xo",
    "update": "tipi update"
  },
  "ava": {
    "babel": "inherit",
    "files": [
      "source/*.test.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "exclude": [
            "transform-regenerator"
          ],
          "targets": {
            "node": 4
          }
        }
      ]
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "tipi": {
    "version": "3.1.0",
    "template": "node"
  },
  "xo": {
    "ignore": [
      "library/**/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marionebl/eslint-plugin-patternplate.git"
  },
  "keywords": [],
  "author": "marionebl <hello@herebecode.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marionebl/eslint-plugin-patternplate/issues"
  },
  "homepage": "https://github.com/marionebl/eslint-plugin-patternplate#readme",
  "devDependencies": {
    "ava": "0.18.2",
    "babel-cli": "6.24.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-env": "1.2.2",
    "babel-register": "6.24.0",
    "mz": "2.6.0",
    "redent": "2.0.0",
    "tipi-cli": "1.0.0",
    "xo": "0.16.0"
  },
  "dependencies": {
    "babel-generator": "6.24.0",
    "babel-traverse": "6.23.1",
    "babel-types": "6.23.0",
    "babylon": "6.16.1",
    "babylon-ast-dependencies": "1.0.2",
    "common-tags": "1.4.0",
    "cst": "0.4.9",
    "estraverse": "4.2.0",
    "load-json-file": "2.0.0",
    "path-exists": "3.0.0",
    "resolve-from": "2.0.0"
  }
}
